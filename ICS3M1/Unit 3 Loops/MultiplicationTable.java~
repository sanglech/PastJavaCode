public class MultiplicationTable
{ 
  public static void main (String[] args)
  { 
   
    System.out.println (" Enter your integer");
   int i = ReadLib.readInt();
    int x;
 
    for (int i = 0; i < 13 ; i++)
   x = i;
   x*= i;
  
 System.out.println ( i + "times" + x);
  }
  
  
}
                      