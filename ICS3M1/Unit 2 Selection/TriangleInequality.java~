public class TriangleInequality
{ 
  public static void main (String[] args)
    
  { System.out.println (" Imput value for side a ");
     int a = ReadLib.readInt();
  
  System.out.println (" Imput value for side b ");
    int b = ReadLib.readInt();
  
     System.out.println (" Imput value for side c ");
    int c = ReadLib.readInt();
    
    if (( a + b) > c) 
    { 
      System.out.println ( " So far so good ");
    
    }
    
    else if ( a + c > b )
    
      { 
      System.out.println ( " Almost there..... ");
    
    }
    
    else if ( b + c > a )
        { 
      System.out.println ( " Hurray your done =D ");
    
    }

    
    
    
    
    
    
    
    
    
  }
  
  
  
  
  
  
}
    
    
    
   